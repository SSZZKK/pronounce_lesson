<div class="outline-box">
 <div class="text-center font-default">
　<div class="admins-new-wrapper">
   <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4 users-new-container">
        <h1 class="text-center">ログイン</h1>
        <%= form_for :session, url: login_path do |f| %>
          <div class="form-group">
            <%= f.label :id, class: "text-white" %>
            <%= f.text_field :admin_id, class: "form-control" %>
          </div>
          <div class="form-group">
            <%= f.label :password, class: "text-white" %>
            <%= f.password_field :password, class: "form-control" %>
          </div>

          <%= f.submit "⚒️", class: "btn-white" %>
        <% end %>
       </div>
      </div>
     </div>
    </div>
  </div>
</div>