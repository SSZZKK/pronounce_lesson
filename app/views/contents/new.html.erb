<div class="outline-box">
 <div class="top-hero-container font-default">
  <div class="content-new-wrapper" >
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <h1 class="text-center" "title_letter">ファイルアップロード</h1>
        <%= form_for @content do |f| %>
          <div class="form-group">
            <%= f.label :video %>
            <%= f.file_field :video %>
          </div>

          <div class="form-group">
            <%= f.label :description %>
            <%= f.text_area :description, class: 'form-control' %>
          </div>

          <%= f.submit '送信', class: "btn-white" %>

        <% end %>
        </div>
       </div>
      </div>
    </div>
  </div>
</div>

